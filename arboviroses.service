[Unit]
Description=Arboviroses System
After=docker.service
Requires=docker.service

[Service]
Type=oneshot
RemainAfterExit=yes
WorkingDirectory=/home/pedro/projetos/Arboviroses/ProjetoArboviroses
ExecStart=/usr/bin/docker-compose -f docker-compose.full.yml up -d
ExecStop=/usr/bin/docker-compose -f docker-compose.full.yml down
TimeoutStartSec=0

[Install]
WantedBy=multi-user.target
